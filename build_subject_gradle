#!/bin/bash
script_dir="$( cd "$( dirname "${BASH_SOURCE[0]}" )" &> /dev/null && pwd )"
benchmark_name=$(echo $script_dir | rev | cut -d "/" -f 3 | rev)
project_name=$(echo $script_dir | rev | cut -d "/" -f 2 | rev)
bug_id=$(echo $script_dir | rev | cut -d "/" -f 1 | rev)
dir_name=/experiment/$benchmark_name/$project_name/$bug_id
cp /setup/$benchmark_name/$project_name/$bug_id/com.gzoltar.cli-1.7.4-SNAPSHOT-jar-with-dependencies.jar /TBar/gzoltar/com.gzoltar.cli/target
cp /setup/$benchmark_name/$project_name/$bug_id/com.gzoltar.agent.rt-1.7.4-SNAPSHOT-all.jar /TBar/gzoltar/com.gzoltar.agent.rt/target
cd $dir_name/src

<REPLACE>